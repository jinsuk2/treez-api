{
  "name": "treez-api",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build &&./node_modules/.bin/tsc -p .",
    "buildWin": "rimraf build && tsc",
    "debug": "node --inspect-brk ./build/app.js",
    "start": "node ./build/index.js",
    "test": "jest --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinsukkim94/treez-api.git"
  },
  "author": "Jinsuk Kim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jinsukkim94/treez-api/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "/__tests__/fixtures/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    }
  },
  "homepage": "https://github.com/jinsukkim94/treez-api#readme",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.1",
    "@types/mongodb": "^3.3.15",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.7.0",
    "@types/sinon": "^7.5.1",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "ts-mongoose": "0.0.21"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "lodash": "^4.17.21",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "rimraf": "^3.0.1",
    "sinon": "^8.1.1",
    "typescript": "^3.7.5"
  }
}
